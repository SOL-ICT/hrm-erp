ESCRIBE recruitment_requests;
SELECT COLUMN_NAME 
FROM INFORMATION_SCHEMA.COLUMNS 
WHERE TABLE_NAME = 'recruitment_requests' 
AND TABLE_SCHEMA = 'hrm_database'
AND COLUMN_NAME LIKE '%service%';
